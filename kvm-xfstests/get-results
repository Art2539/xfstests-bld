#!/bin/sh

REGEXP="( Linux version )|(^FSTEST)|(^BEGIN)|(^MOUNT_OPTIONS)|(^MKFS_OPTIONS)|(^END)|(^EXT4-fs error)|(WARNING)|(^Ran: )|(^Failures: )|(^Passed)|(inconsistent)"

case "$1" in
    --failures|-F)
	REGEXP="(^BEGIN)|(^Failures: )|(^Passed)"
	do_failure="yes"
	shift
	;;
esac

IN="$*"

if test -z "$IN"
then
    IN=$(/bin/ls -t log.* | head -1)
fi

egrep "$REGEXP" "$IN"
if test -n "$do_failure"; then
    b=$(egrep ^BEGIN $IN | wc -l)
    e=$(egrep ^END $IN | wc -l)
    if test $b -gt $e ; then
	l=$(egrep ^BEGIN $IN | tail -n 1 | sed -e 's/BEGIN //')
	echo " "
	echo Missing END $l
    fi
fi
